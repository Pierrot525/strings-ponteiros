#include <stdio.h>
#include <string.h>

char letras(char x[50]){
                
            for (int i=0;i<4;i++){
                   
                   printf("%c", x[i]);                
            }    
}

int main(){
char nome[50];

         printf("digite seu nome\n");
           fgets(nome,50,stdin);
           
          letras(nome);

    return 0;
}
