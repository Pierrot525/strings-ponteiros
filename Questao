#include <stdio.h>
#include <string.h>

int main(){
char nome[50];

     printf("digite o seu nome\n");
       fgets(nome,50,stdin);

         if(nome[0]=='a' || nome[0]=='A'){
           printf("seu nome e: %s",nome);
           }

         else{          
           printf("errado baitola");
           }     
         
    return 0;
}
